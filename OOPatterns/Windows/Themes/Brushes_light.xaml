    <SolidColorBrush x:Key="Primary" Color="#DFE8F0"/>
    <SolidColorBrush x:Key="PrimaryDark" Color="#A3B7C9"/>
    <SolidColorBrush x:Key="PrimaryLight" Color="White"/>
    <SolidColorBrush x:Key="PrimaryForeground" Color="Black"/>

    <SolidColorBrush x:Key="ControlBorder" Color="#959B9E"/>
    <SolidColorBrush x:Key="DisabledForeground" Color="#888888" />

    <SolidColorBrush x:Key="PrimaryButton" Color="#CFCFCF" />
    <SolidColorBrush x:Key="OverButton" Color="#C5C5C5" />
